{
  "name": "wce-appli-bureau",
  "productName": "WebConf de l'État",
  "version": "1.0.0",
  "description": "WebConf de l'État",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DISIC/wce-appli-bureau"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.webconf",
    "productName": "webconf",
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "fav.ico"
    }
  },
  "nsis": {
    "oneClick": true,
    "installerIcon": "fav.ico",
    "uninstallerIcon": "fav.ico",
    "uninstalleDisplayName": "uninstall webconf",
    "allowToChangeInstallationDirectory": true
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@jitsi/electron-sdk": "^5.0.3",
    "auto-updater": "^1.0.2",
    "dotenv": "^16.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.1"
  },
  "devDependencies": {
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3"
  }
}